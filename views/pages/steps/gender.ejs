<div class="heading">
  <h2>What is your gender?</h2>
</div>
<form class="step step4">
  <div class="form-check">
    <input class="form-check-input" type="radio" name="gender" id="female" value="Female" onclick="setGenderData();">
    <label class="form-check-label" for="female">
      <img src="images/female-icon.svg" alt="step-icon"/>
      Female
    </label>
  </div>
  <div class="form-check">
    <input class="form-check-input" type="radio" name="gender" id="male" value="Male" onclick="setGenderData();">
    <label class="form-check-label" for="male">
      <img src="images/male-icon.svg" alt="step-icon"/>
      Male
    </label>
  </div>
</form>
